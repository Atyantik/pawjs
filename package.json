{
  "name": "@pawjs/pawjs",
  "version": "2.1.1",
  "description": "A minimal pluggable reactjs framework to develop a progressive web application",
  "main": "scripts.js",
  "scripts": {
    "build": "npm run build:prod",
    "build:dev": "node ./paw.js --no-cache --root=./demo --env=development --verbose build",
    "build:prod": "node ./paw.js --no-cache --root=./demo --env=production build",
    "lint": "eslint -c ./.eslintrc --ignore-path ./.gitignore ./src",
    "lint:fix": "eslint -c ./.eslintrc --ignore-path ./.gitignore --fix ./src",
    "publish-npm:beta": "npm publish --tag=next",
    "start": "npm run start:dev",
    "start:dev": "node ./paw.js --no-cache --root=./demo --env=development --verbose start",
    "start:prod": "node ./paw.js --no-cache --root=./demo --env=production start",
    "test": "cross-env PROJECT_ROOT=./demo node ./paw.js test"
  },
  "bin": {
    "paw": "./paw.js",
    "pawjs": "./paw.js"
  },
  "keywords": [
    "framework",
    "pawjs",
    "react",
    "pwa",
    "progressive web application",
    "webpack4",
    "pluggable",
    "react-pwa"
  ],
  "author": "Atyantik Technologies Private Limited <tirthbodawala@atyantik.com>",
  "license": "MIT",
  "dependencies": {
    "@babel/cli": "^7.0.0-beta.52",
    "@babel/core": "^7.0.0-beta.52",
    "@babel/plugin-proposal-async-generator-functions": "^7.0.0-beta.52",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.52",
    "@babel/plugin-proposal-decorators": "^7.0.0-beta.52",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.52",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0-beta.52",
    "@babel/polyfill": "^7.0.0-beta.52",
    "@babel/preset-env": "^7.0.0-beta.52",
    "@babel/preset-react": "^7.0.0-beta.52",
    "@babel/register": "^7.0.0-beta.52",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.2.5",
    "babel-jest": "^23.4.0",
    "babel-loader": "^8.0.0-beta.4",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-template": "^6.26.0",
