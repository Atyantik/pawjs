    "react": "^16.4.1",
    "pawjs": "^2.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "cpr": "^3.0.1",
    "react": "^16.4.2",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
    "regenerator-runtime": "^0.12.1"
  },
  "main": "src/firebase-server.js",
  "browser": "src/firebase-client.js",
